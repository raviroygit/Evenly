# Evenly App Environment Variables
# Copy this file to .env and update the values

# API URLs
EXPO_PUBLIC_AUTH_SERVICE_URL=https://your-auth-service-url.com/api/v1/auth
EXPO_PUBLIC_EVENLY_BACKEND_URL=https://your-backend-url.com/api

# VoAgents API Configuration
EXPO_PUBLIC_VOAGENTS_API_URL=https://agents.voagents.ai
EXPO_PUBLIC_VOAGENTS_AGENT_ID=your-agent-id-here

# App Configuration
EXPO_PUBLIC_APP_NAME=Evenly
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment
NODE_ENV=development
