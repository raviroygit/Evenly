{
  "name": "evenly",
  "version": "1.0.3",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild && node scripts/android-release-signing.js",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "android:apk": "cd android && rm -rf app/.cxx app/build build .gradle && ./gradlew assembleRelease && cd ..",
    "android:bundle": "cd android && rm -rf app/.cxx app/build build .gradle && ./gradlew bundleRelease && cd .."
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.8",
    "axios": "^1.12.2",
    "babel-preset-expo": "~54.0.0",
    "eventemitter3": "^5.0.4",
    "expo": "54.0.20",
    "expo-asset": "~12.0.9",
    "expo-background-fetch": "~14.0.9",
    "expo-blur": "~15.0.7",
    "expo-glass-effect": "~0.1.4",
    "expo-image-manipulator": "^14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-navigation-bar": "~5.0.9",
    "expo-router": "~6.0.13",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "~14.0.9",
    "expo-video": "~3.0.15",
    "i18next": "^25.8.4",
    "nativewind": "4.1.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-super-grid": "^6.0.2",
    "react-native-svg": "15.12.1",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
